{
  "name": "shaunfreeman/uthando-cms",
  "description": "Basic blog application",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [
    "framework",
    "mvc",
    "zf"
  ],
  "homepage": "http://uthando-cms.co.uk/",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1",
    "zendframework/zend-component-installer": "^1.0 || ^0.7 || ^1.0.0-dev@dev",
    "zfcampus/zf-development-mode": "^3.0",
    "zendframework/zend-mvc": "^3.1",
    "zendframework/zend-navigation": "^2.8",
    "zendframework/zend-paginator": "^2.7",
    "zendframework/zend-hydrator": "^2.2",
    "zendframework/zend-inputfilter": "^2.7",
    "zendframework/zend-form": "^2.10",
    "zendframework/zend-db": "^2.8",
    "zendframework/zend-session": "^2.8",
    "zendframework/zend-mvc-plugin-flashmessenger": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Admin\\": "module/Admin/src/",
      "Blog\\": "module/Blog/src/",
      "Core\\": "module/Core/src/",
      "User\\": "module/User/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AdminTest\\": "module/Admin/test/",
      "BlogTest\\": "module/Blog/test/",
      "CoreTest\\": "module/Core/test/",
      "UserTest\\": "module/User/test/"
    }
  },
  "scripts": {
    "cs-check": "phpcs module/Application/src",
    "cs-fix": "phpcbf module/Application/src",
    "development-disable": "zf-development-mode disable",
    "development-enable": "zf-development-mode enable",
    "development-status": "zf-development-mode status",
    "post-create-project-cmd": [
      "@development-enable"
    ],
    "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
    "test": "phpdbg -qrr ./vendor/bin/phpunit -v"
  },
  "require-dev": {
    "zendframework/zend-developer-tools": "^1.1",
    "zendframework/zend-test": "^3.1",
    "zendframework/zendservice-twitter": "^2.0",
    "zendframework/zendservice-akismet": "^2.0",
    "codeclimate/php-test-reporter": "^0.4.4",
    "squizlabs/php_codesniffer": "^3.0",
    "summernote/summernote": "^0.8",
    "ccampbell/chromephp": "^4.0",
    "studio-42/elfinder": "^2.0",
    "ezyang/htmlpurifier": "^4.9"
  }
}
